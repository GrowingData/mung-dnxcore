	/* No layout, just brackets please */
	Layout = null;

	var dashboard = ViewBag.Dashboard as Dashboard;
	var graph = ViewBag.Graph as Graph;
	var title = "";
	if (graph != null) {
		title = graph.Name;

	}

	var bracketsPath = "/lib/brackets/";
}<!doctype html>

